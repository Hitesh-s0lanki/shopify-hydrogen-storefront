{
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm install",
  "outputDirectory": "dist/client",
  "functions": {
    "api/**/*.js": {
      "includeFiles": "{dist/**,app/**}"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
